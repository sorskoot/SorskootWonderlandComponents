{
    "name": "@sorskoot/wonderland-components",
    "version": "1.6.0",
    "description": "Collection of personal components for Wonderland Engine",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sorskoot/SorskootWonderlandComponents.git"
    },
    "bugs": {
        "url": "https://github.com/sorskoot/SorskootWonderlandComponents/issues"
    },
    "homepage": "https://timmykokke.com",
    "wonderlandengine": {},
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./atomECS": "./dist/atomECS/index.js",
        "./react": {
            "types": "./dist/ui/index.d.ts",
            "default": "./dist/ui/index.js"
        }
    },
    "bin": {
        "cbor-encode": "bin/cbor-encode"
    },
    "scripts": {
        "build": "npm run build:code && npm run build:tools",
        "build:tools": "tsc -p tsconfig-tools.json",
        "build:code": "tsc",
        "build:watch": "tsc --watch",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "prepack": "npm run build"
    },
    "files": [
        "dist/**/*.d.ts",
        "dist/**/*.js",
        "shaders/*.*"
    ],
    "keywords": [
        "wonderland-engine",
        "wonderland",
        "sorskoot",
        "webxr",
        "components"
    ],
    "author": "Timmy Kokke / Sorskoot",
    "license": "MIT",
    "dependencies": {
        "gl-matrix": "^3.4.3"
    },
    "devDependencies": {
        "@types/react": "^19.1.1",
        "react": "^19.1.0",
        "typescript": "^5.6.3",
        "@types/node": "^22.10.5",
        "cbor2": "^2.0.1",
        "@wonderlandengine/api": "file:../../Tools/Wonderland Nightly/js/api/wonderlandengine-api.tgz"
    },
    "peerDependencies": {
        "@wonderlandengine/api": "^>=1.3.0",
        "@wonderlandengine/components": ">=1.2.0",
        "@wonderlandengine/react-ui": ">=0.2.4",
        "react": ">=16.8.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "@wonderlandengine/react-ui": {
            "optional": true
        }
    }
}
